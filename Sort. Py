# Sorting elements in Python

# Example list
numbers = [5, 2, 9, 1, 7, 3]

# Ascending order
ascending = sorted(numbers)
print("Ascending:", ascending)

# Descending order
descending = sorted(numbers, reverse=True)
print("Descending:", descending)

# In-place sorting
numbers.sort()
print("In-place Ascending:", numbers)
